<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<SplitPane dividerPositions="0.75" prefHeight="814.0" prefWidth="1472.0" xmlns="http://javafx.com/javafx/8"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ConvergencePictureController">
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="640.0" prefWidth="400.0">
        <Canvas fx:id="convImg" height="802.0" layoutX="44.0" layoutY="42.0" onMouseMoved="#drawPath"
                width="802.0"/>
        <Canvas fx:id="wayImg" height="802.0" layoutX="44.0" layoutY="42.0" onMouseMoved="#drawPath"
                width="802.0"/>
    </AnchorPane>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
        <FlowPane layoutX="15.0" layoutY="128.0" prefHeight="455.0" prefWidth="230.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <Button fx:id="convButton" mnemonicParsing="false" onAction="#drawConvergencePicture" text="Draw"/>
        </FlowPane>
    </AnchorPane>
</SplitPane>
